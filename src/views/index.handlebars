<div>
  <h1>{{title}}</h1>
</div>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Producto</th>
      <th scope="col">Código</th>
      <th scope="col">Descripción</th>
      <th scope="col">stock</th>
    </tr>
  </thead>
  {{#each products}}
  <tbody>
    <tr>
      <th scope="row">{{products.title}}</th>
      <td>{{products.code}}</td>
      <td>{{products.description}}</td>
      <td>{{products.stock}}</td>
    </tr>
  </tbody>
</table>
  {{/each}}

