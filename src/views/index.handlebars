<div>
  <h1>{{title}}</h1>
</div>
<table class="table">
  <thead>
    <tr class="table-primary">
      <th scope="col">Producto</th>
      <th scope="col">Código</th>
      <th scope="col">Descripción</th>
      <th scope="col">stock</th>
    </tr>
  </thead>
  <tbody>
  {{#each products}}
    <tr class="table-secondary">
      <th scope="row">{{this.title}}</th>
      <td>{{this.code}}</td>
      <td>{{this.description}}</td>
      <td>{{this.stock}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>

